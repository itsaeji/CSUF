        <!-- Place button inside grid. -->
        <Button Grid.Column="1" HorizontalAlignment="Stretch" Grid.Row="19" VerticalAlignment="Stretch" Click="BackButton_IsClicked" Style="{StaticResource NavigationBackButtonNormalStyle}" FontSize="36"/>
        <Button x:Name="Table1" Content="1" Grid.Column="5" Grid.RowSpan="3"  HorizontalAlignment="Stretch" Margin="0,0,0,0" Grid.Row="1" VerticalAlignment="Stretch" FontSize="48" Click="Table_IsClicked" BorderThickness="0">
            <Button.Foreground>
                <SolidColorBrush Color="White"/>
            </Button.Foreground>
            <Button.Background>
                <SolidColorBrush Color="Red" Opacity="0"/>
            </Button.Background>
        </Button>
        <Button x:Name="Table2" Content="2" Grid.Column="8" Grid.RowSpan="3"  HorizontalAlignment="Stretch" Margin="0,0,0,0" Grid.Row="1" VerticalAlignment="Stretch" Grid.ColumnSpan="2" FontSize="48" Click="Table_IsClicked" BorderThickness="0">
